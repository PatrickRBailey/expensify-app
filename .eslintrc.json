{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "globals": {
    "React": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "no-case-declarations":"off",
    "strict": [2, "never"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2
  },
  "plugins": [
    "react"
  ]
}